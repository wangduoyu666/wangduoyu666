//This code should be altered, because it's outcome is not correct.

void dot(char *a, int alen)
{
      int c;
      char ctest[c];
      ctest[c]= (char *)malloc(100);
      for(alen=0; alen<c;alen++){
              a[alen]=rand()%c+'a';
      }
      printf("%c", a[alen]);
}

void star(char *b, int blen)
{
      int d;
      char dtest[d];
      dtest[d]=(char *)malloc(100);
      dot(dtest, d);
      for(blen=0; blen<d; blen++){
             b[blen]=rand()%d+'a';
      }
     printf("%s", b[blen]); 
}


int main(void)
{
   int e;
   char etest[e];
   etest[e]=(char *)malloc(100);
   etest[e]="abcdhijkqagt";
   int f;
   char ftest[f];
   ftest[f]=(char *)malloc(100);
   dot(etest, e);
   printf("%c", etest[e]);
   star(ftest, f);
   printf("%s", ftest[f]);
   return(0);
}
