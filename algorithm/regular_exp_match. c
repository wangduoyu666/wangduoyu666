//This code should be altered, because I transformed the other's java code.

#include <stdio.h>
#include <string.h>
#include <stdbool.h>


bool ismatch(char *s, char *p)
{
    int m=strlen(s)+1;
    int n=strlen(p)+1;
    if(s==NULL || p==NULL){
            return true;
    }
    if(s==NULL && strlen(p)==1){
            return false;
    }
    int rows, cols;
    int dp[rows][cols];
    dp[0][0]=true;
    int i,j;
    int r,c;
    for(int r=1; r<m; r++){
            int i=r-1;
            for(int c=1; c<n; c++){
                    int j=c-1;
                    if(s[i]==p[j] ||p[j]=='.'){
                            dp[r][c]=dp[r-1][c-1];
                    }
                    else if
                            (p[j]=='*'){
                                    if(p[j]==s[i] ||p[j-1]=='.'){
                                            dp[r][c]=dp[r-1][c] || dp[r][c-2];
                                    }else{
                                            dp[r][c]=dp[r][c-2];
                                    }
                            }
                    else{
                            dp[r][c]=false;
                    }
            }
    }
    return dp[m-1][n-1];
}

int main(void)
{

}
