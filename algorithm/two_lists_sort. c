#include <stdio.h>

int main(void){
struct ListNode* mergeTwoLists(struct ListNode* a, struct ListNode* b){
    
    if(a==NULL){
        return b;
        }
        else if(b==NULL){
            return a;
        }
 else  if(a->val <b->val){
                a->next=mergeTwoLists(a->next, b);
                return a;
                
            }
            else{
                b->next=mergeTwoLists(a,b->next);
                return b;
            }
        }
        }
        
