 // I can't think something new about this 
 
 #include<stdio.h>
 #include<stdlib.h>
 
 struct list {
    int val;
     struct list *next;
  };


struct list *mergetwo(struct list *a, struct list *b){
    struct list * head=malloc(sizeof(struct list));
    struct list *tail = head;
    if(a==NULL || b==NULL){
        return a?a:b;
    }
    
    while(a!=NULL && b!=NULL){
        if(a->val < b->val){
            tail->next=a;
            a=a->next;
        }
        else{
            tail->next=b;
            b=b->next;
        }
        tail=tail->next;
        }
        tail->next=(a !=NULL?a:b);
        return head->next;
    }
    
    



struct list *mergeKLists(struct list** total, int num){
    struct list *to=NULL;
    for(int i=0; i<num; i++)
    to=mergetwo(to,total[i]);
    return to;
}
